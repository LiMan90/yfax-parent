<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfax.webapi.qmtt.vo.IpShareCodeVo">

	<select id="selectIpShareCodeByIp" resultType="com.yfax.webapi.qmtt.vo.IpShareCodeVo">
        select * from qmtt_ip_sharecode where sourceIp=#{sourceIp} limit 1
    </select>

    <!-- 添加 -->
	<insert id="insertIpShareCode" parameterType="com.yfax.webapi.qmtt.vo.IpShareCodeVo">
		insert into qmtt_ip_sharecode
		<trim prefix="(" suffix=")" suffixOverrides="," >
		  <if test="id != null" > id, </if>
		  <if test="sourceIp != null" > sourceIp, </if>
		  <if test="shareCode != null" > shareCode, </if>
		  <if test="isUsed != null" > isUsed, </if>
		  <if test="createDate != null" > createDate, </if>
		  <if test="updateDate != null" > updateDate, </if>
		  
		</trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
		  <if test="id != null" > #{id}, </if>
		  <if test="sourceIp != null" > #{sourceIp}, </if>
		  <if test="shareCode != null" > #{shareCode}, </if>
		  <if test="isUsed != null" > #{isUsed}, </if>
		  <if test="createDate != null" > #{createDate}, </if>
		  <if test="updateDate != null" > null, </if>
		  
        </trim>
	</insert>
</mapper>
