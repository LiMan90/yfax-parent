<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfax.webapi.ytt.vo.WithdrawHisVo">

    <select id="selectWithdrawHis" resultType="com.yfax.webapi.ytt.vo.WithdrawHisVo">
		select * from ytt_withdraw_his where phoneNum = #{phoneNum} order by withdrawTime desc limit 200
    </select>
    
    <!-- 添加 -->
	<insert id="insertWithdrawHis" parameterType="com.yfax.webapi.ytt.vo.WithdrawHisVo">
		insert into ytt_withdraw_his
		<trim prefix="(" suffix=")" suffixOverrides="," >
		  <if test="id != null" > id, </if>
		  <if test="phoneNum != null" > phoneNum, </if>
		  <if test="withdrawType != null" > withdrawType, </if>
		  <if test="withdrawName != null" > withdrawName, </if>
		  <if test="name != null" > name, </if>
		  <if test="account != null" > account, </if>
		  <if test="income != null" > income, </if>
		  <if test="withdrawTime != null" > withdrawTime, </if>
		  <if test="status != null" > status, </if>
		  <if test="statusType != null" > statusType, </if>
		  <if test="createDate != null" > createDate, </if>
		  <if test="updateDate != null" > updateDate, </if>
		  
		</trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
		  <if test="id != null" > #{id}, </if>
		  <if test="phoneNum != null" > #{phoneNum}, </if>
		  <if test="withdrawType != null" > #{withdrawType}, </if>
		  <if test="withdrawName != null" > #{withdrawName}, </if>
		  <if test="name != null" > #{name}, </if>
		  <if test="account != null" > #{account}, </if>
		  <if test="income != null" > #{income}, </if>
		  <if test="withdrawTime != null" > #{withdrawTime}, </if>
		  <if test="status != null" > #{status}, </if>
		  <if test="statusType != null" > #{statusType}, </if>
		  <if test="createDate != null" > #{createDate}, </if>
		  <if test="updateDate != null" > null, </if>
		  
        </trim>
	</insert>
</mapper>
